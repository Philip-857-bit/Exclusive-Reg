generator client {
  provider = "prisma-client-js"
}

datasource db {
  provider = "postgresql"
  url      = env("DATABASE_URL")
}

model Registration {
  id      String @id @default(uuid())
  name    String
  email   String
  phone   String
  company String
  createdAt DateTime @default(now())
  @@unique([email, name, phone, company])
}

model Investor {
  id        Int      @id @default(autoincrement())
  brand     String
  contact   String
  email     String
  phone     String
  website   String
  logo      String
  createdAt DateTime @default(now())
}
generator client {
  provider = "prisma-client-js"
}

datasource db {
  provider = "postgresql"
  url      = env("DATABASE_URL")
}

model Registration {
  id        Int      @id @default(autoincrement())
  name      String
  email     String
  phone     String
  company   String
  createdAt DateTime @default(now())
}

model Investor {
  id        Int      @id @default(autoincrement())
  name      String
  email     String
  firm      String?
  message   String?
  createdAt DateTime @default(now())
}
